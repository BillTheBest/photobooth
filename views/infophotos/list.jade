extends ../layout

block content
	div#content-wrapper
		div#banner.center
			div#bannerImage
				img(src='../../images/EMC-Code-Icon.png')
			div#bannerText
				h1 Choose Your Name To Get Started
		div.clearfix
		ul#list
			- each infophoto, i in infophotos
				li
					div#itemLeft
						h2.
							#{infophoto.fname} #{infophoto.lname}
					div#itemRight
						a(href='/infophotos/takepic/#{infophoto.uniqueurl}' class='btn') Take Photobooth Photos!

